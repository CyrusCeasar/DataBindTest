<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <com.example.chenlei2.databindtest.ui.plugin.custom.CustomToolBar
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/led_config_ctrl"
        android:orientation="vertical"
        app:left_tv_visible="true"
        app:left_tv_text="返回"
        app:right_tv_text="添加"
        app:right_tv_visible="true"

        app:title_visible="true"
        app:title_text="提醒列表"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        tools:context="com.twsz.ivybox.client.account.LedConfigActivity"/>
    <ListView
        android:id="@+id/lv_alarm_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>